IZE = 1000000000
n =int(input())
x = input().split( )
ary = list()
prime = list()
ary[1] = False
i = 2
num =0
while i < SIZE:
    if ary[i] == None:
        prime[num] = True
        num += 1
        j = 2
        while i*j < SIZE:
            ary[i*j] = False
            j += 1


def isPrime(x):
    end = False
    n = 0
    while n < num:
        if prime[n] == num:
            end = True
            break
    print(end)

for i in range(0,n):
    isPrime(int(x[i]))
